{
  "name": "FastAPI練習環境",
  "dockerFile": "./Dockerfile",
  "remoteUser": "vscode",
  "workspaceMount": "type=bind,source=${localWorkspaceFolder},target=/home/vscode/project",
  "workspaceFolder": "/home/vscode/project",
  "extensions": [
    "ms-python.python",
    "vmsynkov.colonize",
